{
  "private": true,
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=1256 PORT=8001  UMI_ENV=dev umi dev ",
    "start:dev": "cross-env PORT=8001 REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "build": "cross-env  umi build",
    "build:esbuild": "cross-env UMI_ENV=esbuild  umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "antd-mobile": "^5.20.0",
    "dayjs": "^1.11.3",
    "mockjs": "^1.1.0",
    "moment": "^2.29.3",
    "react": "17.x",
    "react-dev-inspector": "^1.8.1",
    "react-dom": "17.x",
    "umi": "3.5.26"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/preset-react": "^2.1.4",
    "@umijs/test": "^3.5.23",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "umi-plugin-tailwindcss": "^3.2.1",
    "yorkie": "^2.0.0"
  }
}
