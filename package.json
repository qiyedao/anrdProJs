{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "antd-mobile": "^5.20.0",
    "dayjs": "^1.11.3",
    "mockjs": "^1.1.0",
    "moment": "^2.29.3",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "3.5.26"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/preset-react": "^2.1.4",
    "@umijs/test": "^3.5.23",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
