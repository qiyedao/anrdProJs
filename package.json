{
  "private": true,
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=1256 PORT=8001 COMPRESS=none   UMI_ENV=dev umi dev ",
    "start:no-mock": "cross-env PORT=8001 REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:https": "cross-env NODE_OPTIONS=--max_old_space_size=1256 PORT=8001   UMI_ENV=dev umi dev ",
    "build": "cross-env  umi build",
    "build:esbuild": "cross-env UMI_ENV=esbuild  umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "analyze:ssr": "cross-env ANALYZE_SSR=1 umi build",
    "lint-staged": "lint-staged",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "postinstall": "umi generate tmp",
    "plugins": "umi plugin list --key",
    "prepare": "husky install",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "ahooks": "^3.7.0",
    "antd-mobile": "^5.20.0",
    "autoprefixer": "9.x",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.3",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "react": "17.x",
    "react-dev-inspector": "^1.8.1",
    "react-dom": "17.x",
    "umi": "^3.5.34"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/preset-react": "^2.1.4",
    "@umijs/test": "^3.5.23",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "husky": "^8.0.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
